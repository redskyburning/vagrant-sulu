{% block content %}
    <div class="template-styles" id="content" vocab="http://schema.org/" typeof="Content">

        <div class="template-styles__header container">
            <div class="page-header">
                <h1 class="template-styles__title">Style Guide</h1>
            </div>
        </div>

            <div class="template-styles__content">

                <section class="template-styles__section container" id="colors">

                    <h2 class="template-styles__section__title">Colors</h2>

                    <div class="template-styles__section__body">


                        <div class="template-styles__color-swatches">

                            {% for key, label in colors %}
                                {% include "ClientWebsiteBundle:components:color_swatch.html.twig" with {
                                'key' : key,
                                'label' : label
                                } %}
                            {% endfor %}

                        </div>

                        <h3 class="template-styles__section__subtitle">Semantic Colors</h3>

                        <div class="template-styles__color-swatches">

                            {% for key, label in semantic_colors %}
                                {% include "ClientWebsiteBundle:components:color_swatch.html.twig" with {
                                'key' : key,
                                'label' : label
                                } %}
                            {% endfor %}

                        </div>

                    </div>
                </section>

                <section class="container template-styles__section" id="buttons">

                    <h2 class="template-styles__section__title">Buttons</h2>

                    <div class="template-styles__section__body">

                        {% for size in button_sizes %}

                            {% if loop.first == false %}
                                <h3 class="template-styles__section__subtitle">{{ size.name }} Buttons</h3>{% endif %}
                            <div class="template-styles__button-group">
                                {% for bt in button_types %}
                                    <a href="#" class="btn btn{{ size.code }} btn-{{ bt }}">{{ bt }}</a>
                                {% endfor %}
                            </div>

                        {% endfor %}

                    </div>

                </section>

                <section class="container template-styles__section" id="dropdowns">

                    <h2 class="template-styles__section__title">Dropdowns</h2>

                    <div class="template-styles__section__body">
                        <div class="template-styles__dropdown-group">
                            {% for bt in button_types %}
                                <div class="dropdown template-styles__dropdown">
                                    <button class="btn btn-{{ bt }} dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                                        {{ bt }}
                                        <span class="caret"></span>
                                    </button>
                                    <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                                        <li><a href="#">Action</a></li>
                                        <li><a href="#">Another action</a></li>
                                        <li><a href="#">Something else here</a></li>
                                        <li role="separator" class="divider"></li>
                                        <li><a href="#">Separated link</a></li>
                                    </ul>
                                </div>
                            {% endfor %}
                        </div>

                        <h3 class="template-styles__section__subtitle">Dropups</h3>

                        <div class="template-styles__dropdown-group">
                            {% for bt in button_types %}
                                <div class="dropup template-styles__dropup">
                                    <button class="btn btn-{{ bt }} dropdown-toggle" type="button" id="dropdownMenu2" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                        {{  bt }}
                                        <span class="caret"></span>
                                    </button>
                                    <ul class="dropdown-menu" aria-labelledby="dropdownMenu2">
                                        <li><a href="#">Action</a></li>
                                        <li><a href="#">Another action</a></li>
                                        <li><a href="#">Something else here</a></li>
                                        <li role="separator" class="divider"></li>
                                        <li><a href="#">Separated link</a></li>
                                    </ul>
                                </div>
                            {% endfor %}
